{
	"$schema": "./node_modules/@angular/cli/lib/config/schema.json",
	"version": 1,
	"newProjectRoot": "projects",
	"projects": {
		"JayantGoel001": {
			"projectType": "application",
			"schematics": {
				"@schematics/angular:application": {
					"strict": true
				}
			},
			"root": "",
			"sourceRoot": "src",
			"prefix": "app",
			"architect": {
				"build": {
					"builder": "@angular-devkit/build-angular:browser",
					"options": {
						"outputPath": "build",
						"index": "src/index.html",
						"main": "src/main.ts",
						"polyfills": "src/polyfills.ts",
						"tsConfig": "tsconfig.app.json",
						"assets": [
							"src/favicon.ico",
							"src/assets/css",
							"src/assets/fonts",
							"src/assets/images",
							"src/assets/js",
							"src/assets/data.min.js",
							"src/apple-touch-icon.png",
							"src/browserconfig.xml",
							"src/favicon-16x16.png",
							"src/favicon-32x32.png",
							"src/LICENSE",
							"src/manifest.webmanifest",
							"src/robots.txt",
							"src/sitemap_index.xml",
							"src/README.md",
							"src/screenshots"
						],
						"styles": [
							"src/assets/css/font-awesome.min.css",
							"src/assets/css/animate.min.css",
							"src/styles.min.css"
						],
						"scripts": [
							"src/assets/data.min.js",
							"src/assets/js/typed.min.js",
							"src/assets/js/particles.min.js",
							"src/assets/js/vanilla-tilt.min.js"
						],
						"serviceWorker": true,
						"ngswConfigPath": "ngsw-config.json"
					},
					"configurations": {
						"production": {
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "500kb",
									"maximumError": "1mb"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "7kb",
									"maximumError": "10kb"
								}
							],
							"fileReplacements": [
								{
									"replace": "src/environments/environment.ts",
									"with": "src/environments/environment.prod.ts"
								}
							],
							"outputHashing": "all"
						},
						"development": {
							"buildOptimizer": false,
							"optimization": false,
							"vendorChunk": true,
							"extractLicenses": false,
							"sourceMap": true,
							"namedChunks": true
						}
					},
					"defaultConfiguration": "production"
				},
				"serve": {
					"builder": "@angular-devkit/build-angular:dev-server",
					"configurations": {
						"production": {
							"browserTarget": "JayantGoel001:build:production"
						},
						"development": {
							"browserTarget": "JayantGoel001:build:development"
						}
					},
					"defaultConfiguration": "development"
				},
				"extract-i18n": {
					"builder": "@angular-devkit/build-angular:extract-i18n",
					"options": {
						"browserTarget": "JayantGoel001:build"
					}
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"main": "src/test.ts",
						"polyfills": "src/polyfills.ts",
						"tsConfig": "tsconfig.spec.json",
						"karmaConfig": "karma.conf.js",
						"assets": [
							"src/favicon.ico",
							"src/assets",
							"src/manifest.webmanifest"
						],
						"styles": [
							"src/styles.min.css"
						],
						"scripts": []
					}
				},
				"deploy": {
					"builder": "angular-cli-ghpages:deploy",
					"options": {}
				}
			}
		}
	},
	"cli": {
		"analytics": false
	}
}
